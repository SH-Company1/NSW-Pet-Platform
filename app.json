{
  "expo": {
    "name": "NSW Pet Platform",
    "slug": "nsw-pet-platform",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFA042"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nsw.petplatform"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFA042"
      },
      "package": "com.nsw.petplatform"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "NSW Pet Platform",
      "shortName": "NSW Pet",
      "description": "반려동물을 위한 종합 플랫폼",
      "lang": "ko",
      "scope": "/",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait",
      "themeColor": "#FFA042",
      "backgroundColor": "#FFFFFF"
    }
  }
}
